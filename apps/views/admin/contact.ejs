<!DOCTYPE html>
<html>

<head>
    <title></title>

    <%- include layout/head %>
</head>

<body>

    <div class="container">
        <%- include layout/nav %>

            <div class="panel panel-default">
                <div class="panel-heading">
                    Contact
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>Full Name</th>
                                    <th>Email</th>
                                    <th>Phone</th>
                                    <th>Tiêu Đề</th>
                                    <th>Nội dung</th>

                                </tr>
                            </thead>
                            <tbody>
                                <% if (data && data.contact){%>
                                    <% for(var i = 0; i < data.contact.length; i++){%>
                                        <tr>
                                            <td>
                                                <%= data.contact[i].id%>
                                            </td>
                                            <td>
                                                <%= data.contact[i].fullname%>
                                            </td>
                                            <td>
                                                <%= data.contact[i].email%>
                                            </td>
                                            <td>
                                                <%= data.contact[i].phone%>
                                            </td>
                                            <td>
                                                <%= data.contact[i].tieude%>
                                            </td>
                                            <td>
                                                <%= data.contact[i].noidung%>
                                            </td>


                                        </tr>
                                        <%}%>
                                            <%}%>
                            </tbody>
                        </table>
                    </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.panel-body -->
            </div>
    </div>
    <script type="text/javascript" src="/static/js/contact.js"></script>
</body>

</html>